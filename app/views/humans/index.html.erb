<h1>A list of People</h1>
<p>Below are a list of create peoople</p>

<button><%= link_to 'Add a Person', new_human_path %></button>


<table>
<tr>
  <th>Name</th>
  <th>Age</th>
  <th>Hair Color</th>
  <th>Eye Color</th>
  <th>Gender</th>
  <th>Alive?</th>
  
  
</tr>  
  <% @humans.each do |person|%>
    <tr>
      <td><%= person.name %></td>
      <td><%=  person.age %></td>
      <td><%=  person.hair_color %></td>
      <td><%=  person.eye_color %></td>
      <td><%=  person.gender %></td>
      <td><%=  person.alive %></td>
      <td><%= link_to 'Go to Person', person %></td>
    </tr>
    
  <% end %>

  </table>

